//for-in loop
let obj = {
    "name": "Prateek",
    "role": "Programmer",
    "college": "CDGI indore"
}
for (const key in obj) {
    console.log(key);

}
console.log("HEY. guys welcome back!")

    //for-of loop

    for (const char of "Prateek") {
        console.log(char);

    }
    //do -while loop
    i = 0;
    do {
        console.log(i)
        i++;
    } while (i < 6) 
    //Ex :- do-while loop
    let i = 2;
    do {
        console.log(i);
        i=i+2;
    } while (i<=20);
